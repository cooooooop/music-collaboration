    		<s:MixesList ui:field="mixesList" visible="false"/>
    		<g:HorizontalPanel width="100%" height="5px"/>
    		<s:AudioFilesList ui:field="audioFilesList" visible="false"/>
   		</g:VerticalPanel>
	</g:HorizontalPanel>
</ui:UiBinder> 