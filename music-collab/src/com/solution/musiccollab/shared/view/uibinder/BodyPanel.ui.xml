<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g='urn:import:com.google.gwt.user.client.ui'
        xmlns:s='urn:import:com.solution.musiccollab.shared.view'>
	<ui:style>
		.mainPanel
		{
			background-color			:	#4E6BAE;
		}
		
	</ui:style>
	
	
	<g:HorizontalPanel height="100%" horizontalAlignment="ALIGN_CENTER" styleName="{style.mainPanel}" spacing="5">
   		<s:UsersList ui:field="usersList" visible="false"/>
