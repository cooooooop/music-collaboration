<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g='urn:import:com.google.gwt.user.client.ui'
        xmlns:s='urn:import:com.solution.musiccollab.shared.view'>
	<ui:style>
		.mainPanel
		{
			background-color			:	#4E6BAE;	
		}
	
		.list
		{
			border-style				:	solid;
			border-color				:	#001C5E;
		    background-color            :   #4E6BAE;
		    display						: 	inline-block;
		    color						:	#FFF;
		}
		
	</ui:style>
	
	
	<g:HorizontalPanel width="100%" height="100%" horizontalAlignment="ALIGN_CENTER" styleName="{style.mainPanel}">
   		<s:UsersList ui:field="usersList" styleName="{style.list}" visible="false"/>
   		<g:VerticalPanel>
    		<s:MixesList ui:field="mixesList" styleName="{style.list}" visible="false"/>
    		<s:AudioFilesList ui:field="audioFilesList" styleName="{style.list}" visible="false"/>
   		</g:VerticalPanel>
	</g:HorizontalPanel>
</ui:UiBinder> 